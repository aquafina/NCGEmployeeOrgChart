  Name="VOSearchHierarchy"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Variable
    Name="department_name"
    Kind="where"
    Type="java.lang.String"
    DefaultValue="%">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="100"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT XxEmployeeOrgChartV.HIERARCHY_NAME,
       XxEmployeeOrgChartV.PARENT_POSITION_ID,
       XxEmployeeOrgChartV.FULL_NAME_PARENT,
       XxEmployeeOrgChartV.PARENT_PERSON_ID,
       XxEmployeeOrgChartV.PARENT_POSITION,
       XxEmployeeOrgChartV.SUBORDINATE_POSITION_ID,
       XxEmployeeOrgChartV.CHILD_POSITION,
       XxEmployeeOrgChartV.FULL_NAME_CHILD,
       XxEmployeeOrgChartV.CHILD_PERSON_ID
FROM XX_EMPLOYEE_ORG_CHART_V XxEmployeeOrgChartV
where UPPER(XxEmployeeOrgChartV.HIERARCHY_NAME) like '%'||UPPER(:department_name)||'%'  
and   XxEmployeeOrgChartV.PARENT_POSITION_ID is null
union all
SELECT null,
       null PARENT_POSITION_ID,
       null FULL_NAME_PARENT,
       null PARENT_PERSON_ID,
       null PARENT_POSITION,
       null SUBORDINATE_POSITION_ID,
       null CHILD_POSITION,
       'ALL Departments' FULL_NAME_CHILD,
       null CHILD_PERSON_ID
FROM dual]]>
  </SQLQuery>
